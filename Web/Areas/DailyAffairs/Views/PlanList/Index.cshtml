@*检索区域*@
@section SuperSearch{
    <table class="searchContent">
        <tr>
              <th width="auto">
                资产类型：
            </th>
            <td>
                <select name="RemindRecyle" id="RemindRecyle" class="Combo required">
                    <option value="" selected="selected">--全部--</option>
                    <option value="1">交换机</option>
                    <option value="2">服务器</option>
                    <option value="3">防火墙</option>
                </select>
            </td>
            <td>
                <select name="RemindRecyle" id="RemindRecyle" class="Combo required">
                    <option value="" selected="selected">--全部--</option>
                    <option value="1">2-1-第一排-机架10-1 </option>
                    <option value="2">FWQ-1-1-1 </option>
                </select>
            </td>
            <th width="auto">
                维护期内：
            </th>
             <td>
                <input type="checkbox" checked name="" >
            </td>
            <th width="auto">
                关键字：
            </th>
            <td>
                <input type="text" size="30" placeholder="维护信息/计划主题" name="SearchKey" value="@SearchKey"/>
            </td>
        </tr>
    </table>
}
@*工具条区域*@
@section ToolBar{
    @* @Html.Dialog("Create", 800, 600)
